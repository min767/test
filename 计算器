#include <stdio.h>
int i=0;

int tiqu(char ch[])
{  int n=0;
 while(ch[i]>='0'&&ch[i]<='9')
 {n=n*10+(ch[i]-48);i++;}
 return n;
  }


void fuhao(char ch,int ch2[])
{   
 if(ch=='+')
 ch2[0]=1;
 if(ch=='-')
ch2[1]=1;
 if(ch=='*')
ch2[2]=1;
if(ch=='/')
ch2[3]=1;
i++;
}

          int main()
        {
	char ch[100],ch1[5]="+-*/";
	int ch2[4]={0};
	int sum=0,n=0,m=0,j;
             gets(ch);
             
             while ('\0'!=ch[i])
	{      
	      n=tiqu(ch);
	         
                   fuhao(ch[i],ch2);
	           
	        m=tiqu(ch);
	        
                for(j=0;j<4;j++)
                if(ch2[j]==1)
                 {
                  switch(ch1[j])
                { case '+': sum+=n+m;ch2[j]=0;break;
	    case '-': sum+=n-m;ch2[j]=0;break;
	    case '*': 
	    if(n!=0)
	    {sum+=n*m;ch2[j]=0;break;}
	    else
	     {sum*=m;ch2[j]=0;break;}
	    case '/': 
	    if(n!=0)
	    {sum+=n/m;ch2[j]=0;break;}
	    else
	    {sum/=m;ch2[j]=0;break;}
	   }
	    }    
                n=0,m=0;
	}
	printf("   %d\n",sum);

}


#include"stdio.h"
#include"string.h"
int main()
{
	char s1[100]="1+2+2+1+2+5+4-1-3+4-8";
	int i;
	int res=s1[0]-'0';
	for(i=1;i<=strlen(s1);i++)
	{
		if(s1[i]=='+') res+=s1[++i]-'0';
		else if(s1[i]=='-') res-=s1[++i]-'0';	
	}
	printf("结果为:\n");
	printf("%d \n",res); 
	return 0;
}


#include"stdio.h"
#include"string.h"
int main()
{
	char s1[100]="2*2/4*1/1*2*3/2";
	int i;
	int res=s1[0]-'0';
	for(i=1;i<=strlen(s1);i++)
	{
		if(s1[i]=='*') res*=s1[++i]-'0';
		else if(s1[i]=='/') res/=s1[++i]-'0';	
	}
	printf("结果为:\n");
	printf("%d \n",res); 
	return 0;
}


#include"stdio.h"
#include"string.h"
int main()
{
	char s1[100]="2+2*3+2/2-1",s2[100]="0";
	int i,x=-1,a=0,b=0;
	for(i=0;i<strlen(s1);i++)
	{
		if(s1[i]=='*')
		  {
		  a=s2[x]-'0';
		  b=s1[i+1]-'0';
		  s2[x]=a*b+'0';
		  i++;
		  }
        else if(s1[i]=='/')
          {
		  a=s2[x]-'0';
          b=s1[i+1]-'0';
          s2[x]=a/b+'0';
          i++;
		  }
        else s2[++x]=s1[i];
	}
	int res=s2[0]-'0';
		for(i=1;i<strlen(s2);i++)
	{
		if(s2[i]=='+') res+=s2[++i]-'0';
		else if(s2[i]=='-') res-=s2[++i]-'0';	
	}
	printf("结果为:\n");
	printf("%d \n",res); 
	return 0;
}


#include <stdio.h>
int i=0;

int tiqu(char ch[])
{  int n=0;
 while(ch[i]>='0'&&ch[i]<='9')
 {n=n*10+(ch[i]-48);i++;}
 return n;
  }


void fuhao(char ch,int ch2[])
{   
 if(ch=='+')
 ch2[0]=1;
 if(ch=='-')
ch2[1]=1;
 if(ch=='*')
ch2[2]=1;
if(ch=='/')
ch2[3]=1;
i++;
}

          int main()
        {
	char ch[100],ch1[5]="+-*/";
	int ch2[4]={0};
	int sum=0,n=0,m=0,j;
             gets(ch);
             
             while ('\0'!=ch[i])
	{      
	      n=tiqu(ch);
	         
                   fuhao(ch[i],ch2);
	           
	        m=tiqu(ch);
	        
                for(j=0;j<4;j++)
                if(ch2[j]==1)
                 {
                  switch(ch1[j])
                { case '+': sum+=n+m;ch2[j]=0;break;
	    case '-': sum+=n-m;ch2[j]=0;break;
	    case '*': 
	    if(n!=0)
	    {sum+=n*m;ch2[j]=0;break;}
	    else
	     {sum*=m;ch2[j]=0;break;}
	    case '/': 
	    if(n!=0)
	    {sum+=n/m;ch2[j]=0;break;}
	    else
	    {sum/=m;ch2[j]=0;break;}
	   }
	    }    
                n=0,m=0;
	}
	printf("   %d\n",sum);

}
